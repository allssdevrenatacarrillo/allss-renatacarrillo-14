<?xml version="1.0" encoding="utf-8"?>
<odoo>
    <record id="hr_employee_custom" model="ir.ui.view">
        <field name="name">hr.employee.custom</field>
        <field name="model">hr.employee</field>
        <field name="inherit_id" ref="hr.view_employee_form"/>
        <field name="arch" type="xml">
            <xpath expr="//field[@name='category_ids']" position="before">
                <h2>
                    <field name="_allss_prof_register" placeholder="Registro Profissional"/>
                </h2>
            </xpath>
        </field>
    </record>
</odoo>