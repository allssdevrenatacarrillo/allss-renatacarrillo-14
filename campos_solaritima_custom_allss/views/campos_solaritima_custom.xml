<?xml version="1.0" encoding="utf-8"?>
<data>
    <record id="campos_solaritima_custom" model="ir.ui.view">
        <field name="name">Campos Adicionais Solaritima</field>
        <field name="model">res.partner</field>
        <field name="inherit_id" ref="base.view_partner_form"/>
        <field name="arch" type="xml"> 

        <xpath expr="//field[@name='vat']" position="after">
            <field name="_allss_fornecedor"/>
            <field name="_allss_titular_inst"/>
            <!-- <field name="_allss_nome"/> -->
            <field name="_allss_cpf"/>
            <field name="_allss_cnpj"/>
            <field name="_allss_gps"/>
            <field name="_allss_respon"/>
            <field name="_allss_met_pag"/>
            <field name="_allss_origem"/>
        </xpath>

        <page name="internal_notes" position="after">
            <page name="dados_instalacao" string="Dados da Instalação">
                <form string="Dados da Instalação">
                <group>
                    <group>
                        <field name="_allss_concessionaria"/>
                    </group>

                    <group>
                        <field name="_allss_nome_titular_inst"/>
                        <field name="_allss_sobr_titular_inst"/>
                    </group>

                    <group>
                        <field name="_allss_cpf_inst"/>
                        <field name="_allss_cnpj_inst"/>
                    </group>

                    <group>
                        <field name="_allss_end_inst"/>
                        <field name="_allss_city_inst"/>
                        <field name="_allss_est_inst"/>
                        <field name="_allss_cep_inst"/>
                    </group>

                    <group>
                        <field name="_allss_tp_estab_inst"/>
                        <field name="_allss_unid_consum_inst"/>
                        <field name="_allss_class_inst"/>
                        <field name="_allss_gps_inst"/>
                    </group>

                    <group>
                        <field name="_allss_obs_inst"/>
                    </group>

                </group>
                </form>
            </page>
        </page>

        <page name="dados_instalacao" position="after">
            <page string="Dados para Dimensionamento">
                <form string="Dados para Dimensionamento">
                    <group>
                        <group>
                            <field name="_allss_tp_telhado_dimen"/>
                            <field name="_allss_md_consu_dimen"/>
                            <field name="_allss_qtd_mes_dimen"/>
                        </group>

                        <group>
                            <field name="_allss_consu_dimen_1"/>
                            <field name="_allss_consu_dimen_2"/>
                            <field name="_allss_consu_dimen_3"/>
                            <field name="_allss_consu_dimen_4"/>
                            <field name="_allss_consu_dimen_5"/>
                            <field name="_allss_consu_dimen_6"/>
                            <field name="_allss_consu_dimen_7"/>
                            <field name="_allss_consu_dimen_8"/>
                            <field name="_allss_consu_dimen_9"/>
                            <field name="_allss_consu_dimen_10"/>
                            <field name="_allss_consu_dimen_11"/>
                            <field name="_allss_consu_dimen_12"/>
                        </group>
                    </group>
                </form>
            </page>
        </page>




        </field>
    </record>
</data>