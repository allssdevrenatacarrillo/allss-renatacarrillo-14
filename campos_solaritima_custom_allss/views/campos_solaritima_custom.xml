                            <field name="_allss_consu_dimen_7"/>
                            <field name="_allss_consu_dimen_8"/>
                            <field name="_allss_consu_dimen_9"/>
                            <field name="_allss_consu_dimen_10"/>
                            <field name="_allss_consu_dimen_11"/>
                            <field name="_allss_consu_dimen_12"/>
                        </group>
                    </group>
                </form>
            </page>
        </page>

        <page name="dados_dimensionamento" position="after">
            <page name="fotos_instalacao" string="Fotos da Instalação">
                <form string="Fotos da Instalação">
                    <group>
                        <group>
                            <field name="_allss_frente_fotos_inst"/>
